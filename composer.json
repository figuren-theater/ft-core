        "composer-plugin-api": "^1.1 || ^2.0"
    },
    "extra": {
        "class": "Altis\\Composer\\Plugin",
        "altis": {}
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true
        }
    }
}
